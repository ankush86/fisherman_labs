<h1>Monsters</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>monsters</th>
    </tr>
  </thead>
  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= link_to team.name, team_path(team) %></td>
        <td><%= team.monsters.pluck(:name).join(", ") %></td>
        <td>

        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_team_path, :class => 'btn btn-primary' %>